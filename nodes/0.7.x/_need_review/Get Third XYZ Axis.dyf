    <Dynamo.Nodes.CodeBlockNodeModel type="Dynamo.Nodes.CodeBlockNodeModel" guid="fa84f0ad-398a-4f0d-b244-fece30359de2" nickname="Best Fit Plane" x="443" y="29" isVisible="true" isUpstreamVisible="true" lacing="Disabled" CodeText="XYZs = {Point.Origin(),axis1.AsPoint(),axis2.AsPoint()};&#xA;// Make a plane based on the given axes and the origin&#xA;pl = Plane.ByBestFitThroughPoints(XYZs);&#xA;// The normal of that plane is the vector of the third axis&#xA;vec = pl.Normal();&#xA;Vector.ByCoordinates(1,1,1).Normalized();&#xA;// Set up a nested condition and find the main direction of the vector&#xA;cond1 = Math.Abs(vec.Y) &gt; Math.Abs(vec.X) &amp;&amp; Math.Abs(vec.Y) &gt; Math.Abs(vec.Z);&#xA;cond2 = Math.Abs(vec.X) &gt; Math.Abs(vec.Y) &amp;&amp; Math.Abs(vec.X) &gt; Math.Abs(vec.Z);&#xA;cond2 ? &quot;X&quot; : (cond1 ? &quot;Y&quot; : &quot;Z&quot;);" ShouldFocus="false" />
