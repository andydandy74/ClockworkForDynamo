      <Script>import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk

room = IN0
points = IN1
booleans = list()
for point in points:
	try:
		if room.IsPointInRoom(point):
			booleans.append(1)
		else:
			booleans.append(0)
	except:
		booleans.append(list())
OUT = booleans</Script>
